
{{ "Summary of the registry changes" | ansi_cyan }}

{% set item_types = ["attributes", "metrics", "events", "spans", "resources"] %}
{% for item_type in item_types %}
{% set added_count = ctx.changes[item_type] | selectattr('type', 'equalto', 'added') | count %}
{% set renamed_to_new_count = ctx.changes[item_type] | selectattr('type', 'equalto', 'renamed_to_new') | count %}
{% set renamed_to_existing_count = ctx.changes[item_type] | selectattr('type', 'equalto', 'renamed_to_existing') | count %}
{% set deprecated_count = ctx.changes[item_type] | selectattr('type', 'equalto', 'deprecated') | count %}
{% set removed_count = ctx.changes[item_type] | selectattr('type', 'equalto', 'removed') | count %}
{{ ((item_type | title_case) ~ " changes") | ansi_green }}
    Added: {{ added_count }}
    Renamed to new: {{ renamed_to_new_count }}
    Renamed to existing: {{ renamed_to_existing_count }}
    Deprecated: {{ deprecated_count }}
    Removed: {{ removed_count }}
    
{% endfor %}
